{
  "name": "MKS-Async",
  "version": "0.0.0",
  "description": "Async Greenfield Project",
  "engines": {
    "node": "4.4.7",
    "npm": "3.10.6"
  },
  "scripts": {
    "start": "node server/server.js",
    "postinstall": "bower install && npm run build_codes",
    "monitor_codes": "node server/server.js --testing=${USER}",
    "build_codes": "grunt build",
    "watch_codes": "grunt watch",
    "test_codes": "grunt test",
    "mongo_daemon": "mkdir -p data/db && mongod --dbpath data/db",
    "mongo_cleanup": "rm -rf data/db",
    "json_server": "json-server server/models.json",
    "test_karma": "karma start --browsers PhantomJS",
    "test_mocha": "mocha --watch --reporter spec spec/",
    "lint": "eslint --fix public server"
  },
  "author": "Sergey Potashnikov, Cindy Sun, Dana Li, Tim Shiu",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^1.5.7",
    "chai": "^3.5.0",
    "clone-stats": "0.0.1",
    "expect.js": "^0.3.1",
    "grunt": "^0.4.5",
    "jshint-stylish": "^0.2.0",
    "karma": "^0.12.16",
    "karma-chrome-launcher": "^0.1.4",
    "karma-cli": "0.0.4",
    "karma-jasmine": "^0.1.5",
    "karma-mocha": "^0.1.4",
    "karma-nested-reporter": "^0.1.1",
    "karma-nyan-reporter": "0.0.43",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-unicorn-reporter": "^0.1.4",
    "mocha": "^1.20.1",
    "supertest": "^2.0.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.10.2",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.4.3",
    "bower": "^1.7.9",
    "chalk": "^1.1.3",
    "eslint": "^3.3.1",
    "express": "^4.4.5",
    "grunt": "^0.4.4",
    "grunt-contrib-concat": "^0.3.0",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-mocha-test": "^0.10.0",
    "grunt-nodemon": "^0.2.1",
    "grunt-shell": "^0.6.4",
    "jwt-simple": "^0.2.0",
    "mongoose": "^4.1.0",
    "mongoose-sequence": "^3.0.2",
    "morgan": "^1.1.1",
    "passport": "^0.2.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "request": "^2.36.0",
    "yahoo-finance": "0.2.13"
  }
}
